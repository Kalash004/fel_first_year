<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/17.0.12" xmlns:fx="http://javafx.com/fxml/1" fx:controller="view.controllers.TictactoeController">
  <VBox prefHeight="200.0" prefWidth="156.0">
    <GridPane>
      <columnConstraints>
        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
      </columnConstraints>
      <rowConstraints>
        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
      </rowConstraints>
      <Button fx:id="btn00" mnemonicParsing="false" text="Button" />
      <Button fx:id="btn01" mnemonicParsing="false" text="Button" GridPane.columnIndex="1" />
      <Button fx:id="btn02" mnemonicParsing="false" text="Button" GridPane.columnIndex="2" />
      <Button fx:id="btn10" mnemonicParsing="false" text="Button" GridPane.rowIndex="1" />
      <Button fx:id="btn11" mnemonicParsing="false" text="Button" GridPane.columnIndex="1" GridPane.rowIndex="1" />
      <Button fx:id="btn12"  mnemonicParsing="false" text="Button" GridPane.columnIndex="2" GridPane.rowIndex="1" />
      <Button fx:id="btn20" mnemonicParsing="false" text="Button" GridPane.rowIndex="2" />
      <Button fx:id="btn21" mnemonicParsing="false" text="Button" GridPane.columnIndex="1" GridPane.rowIndex="2" />
      <Button fx:id="btn22" mnemonicParsing="false" text="Button" GridPane.columnIndex="2" GridPane.rowIndex="2" />
    </GridPane>
  </VBox>
</AnchorPane>
