      <Button fx:id="btn10" onAction="#onSpaceClick" mnemonicParsing="false" prefHeight="100" prefWidth="100" styleClass="button" text="" GridPane.rowIndex="1" />
      <Button fx:id="btn11" onAction="#onSpaceClick" mnemonicParsing="false" prefHeight="100" prefWidth="100" styleClass="button" text="" GridPane.columnIndex="1" GridPane.rowIndex="1" />
      <Button fx:id="btn12" onAction="#onSpaceClick" mnemonicParsing="false" prefHeight="100" prefWidth="100" styleClass="button" text="" GridPane.columnIndex="2" GridPane.rowIndex="1" />
      <Button fx:id="btn20" onAction="#onSpaceClick" mnemonicParsing="false" prefHeight="100" prefWidth="100" styleClass="button" text="" GridPane.rowIndex="2" />
      <Button fx:id="btn21" onAction="#onSpaceClick" mnemonicParsing="false" prefHeight="100" prefWidth="100" styleClass="button" text="" GridPane.columnIndex="1" GridPane.rowIndex="2" />
      <Button fx:id="btn22" onAction="#onSpaceClick" mnemonicParsing="false" prefHeight="100" prefWidth="100" styleClass="button" text="" GridPane.columnIndex="2" GridPane.rowIndex="2" />
