<AnchorPane fx:id="rootPane" prefHeight="400.0" prefWidth="600.0" styleClass="main-root light-mode" xmlns="http://javafx.com/javafx/17.0.12" xmlns:fx="http://javafx.com/fxml/1" fx:controller="service_view.controllers.TictactoeController">
  <Button fx:id="returnButton" onAction="#onReturnClick" layoutX="320.0" layoutY="229.0" mnemonicParsing="false" text="Return" />
